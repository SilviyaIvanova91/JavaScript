<div class="card mb-4">
  <img
    class="card-img-top"
    src="{{imageUrl}}"
    alt="Card image cap"
    width="400"
  />
  <div class="card-body">
    <h4 class="card-title">{{title}}</h4>
  </div>

  <div class="card-footer">
    <a href="/course/details/{{_id}}"><button
        type="button"
        class="btn btn-primary"
      >Details</button></a>
    <small class="float-right">{{createdAt}}</small>
  </div>

</div>

